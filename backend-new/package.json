{
  "name": "cityventure-backend",
  "version": "2.0.0",
  "description": "CityVenture Backend API - Sequelize ORM with improved structure",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:setup": "node src/scripts/db-setup.js",
    "db:reset": "node src/scripts/db-reset.js",
    "db:procedures": "node src/scripts/create-procedures.js",
    "db:procedures:create": "node src/scripts/create-procedures.js --create",
    "db:procedures:drop": "node src/scripts/create-procedures.js --drop",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimentalf-vm-modules node_modules/jest/bin/jest.js --coverage --forceExit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "cityventure",
    "tourism",
    "booking",
    "api",
    "sequelize"
  ],
  "author": "CityVenture Team",
  "license": "ISC",
  "dependencies": {
    "@emailjs/nodejs": "^5.0.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "expo-server-sdk": "^3.11.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.8.2",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.3",
    "sequelize": "^6.37.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
